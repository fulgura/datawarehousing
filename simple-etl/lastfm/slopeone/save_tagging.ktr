    <replace_variables>N</replace_variables>
    <sql>insert into f_recommendation(recommendation_id, user_id, artist_id, tag_id, rating, date_hierarchy_id, success)
select nextval(&apos;seq_recommendation&apos;), ?, ?, tag_id1, predicted, ?, cast(tag_id1 = ? as int) 
from
(select 
       tag_id1, sum(freq* (rating + diff)) &#47; sum(freq) as predicted 
from 
       ratings as r, 
       vw_freq_diff as fd
where fd.tag_id2= r.tag_id 
and fd.tag_id1&lt; fd.tag_id2 
and artist_id= ?
and tag_id1 not in (select tag_id from ratings where artist_id= ?) 
group by tag_id1 
order by predicted desc)a
limit 1;</sql>
